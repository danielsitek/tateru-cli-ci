{
  "env": {
    "dev": {
      "data": {
        "app": {
          "environment": "dev"
        }
      }
    },
    "prod": {
      "data": {
        "app": {
          "environment": "prod"
        }
      }
    }
  },
  "options": {
    "data": {
      "app": {
        "environment": "dev",
        "root": "/"
      },
      "title": "Tateru CLI Example",
      "domain": "https://example.com/",
      "description": "Description about CI tests for Tateru CLI."
    },
    "src": "src/twig",
    "ext": "dist"
  },
  "translations": {
    "cs": {
      "src": "src/translations/cs.json",
      "ext": ""
    }
  },
  "pages": {
    "cs": {
      "index": {
        "data": {
          "page": "index"
        },
        "src": "views/index.html.twig",
        "ext": "index.html",
        "minify": [
          "prod"
        ]
      },
      "about": {
        "data": {
          "page": "about",
          "title": "About Page"
        },
        "src": "views/about.html.twig",
        "ext": "about.html",
        "minify": [
          "prod"
        ]
      },
      "robots": {
        "data": {
          "page": "robots_txt"
        },
        "src": "views/robots.txt.twig",
        "ext": "robots.txt"
      },
      "sitemap_xml": {
        "data": {
          "page": "sitemap_xml"
        },
        "src": "views/sitemap.xml.twig",
        "ext": "sitemap.xml"
      },
      "webmanifest_json": {
        "data": {
          "page": "webmanifest_json"
        },
        "src": "views/manifest.json.twig",
        "ext": "manifest.json"
      }
    }
  }
}
